{
  "name": "vue-electron-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "electron:dev": "vite build && electron .",
    "electron:build": "vue-tsc --noEmit && vite build && electron-builder"
  },
  "build": {
    "appId": "com.yourcompany.yourapp",
    "productName": "Tool Download Video",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist",
      "dist-electron"
    ]
  },
  "dependencies": {
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/vue-router": "^2.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.0",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vue-tsc": "^3.0.5",
    "wait-on": "^9.0.1"
  }
}
